{
  "version": 2,
  "builds": [
    {
      "src": "api/gemini.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/gemini",
      "dest": "/api/gemini.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
